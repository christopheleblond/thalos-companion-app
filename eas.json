{
  "cli": {
    "version": ">= 16.4.1",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "env": {
        "FIRESTORE_API_KEY": "FIRESTORE_API_KEY",
        "FIRESTORE_APP_ID": "FIRESTORE_APP_ID",
        "FIRESTORE_AUTH_DOMAIN": "FIRESTORE_AUTH_DOMAIN",
        "FIRESTORE_DATABASE_URL": "FIRESTORE_DATABASE_URL",
        "FIRESTORE_MESSAGING_SENDER_ID": "FIRESTORE_MESSAGING_SENDER_ID",
        "FIRESTORE_PROJECT_ID": "FIRESTORE_PROJECT_ID",
        "FIRESTORE_STORAGE_BUCKET": "FIRESTORE_STORAGE_BUCKET"
      }
    },
    "preview": {
      "distribution": "internal"
    },
    "production": {
      "autoIncrement": true
    }
  },
  "submit": {
    "production": {}
  }
}